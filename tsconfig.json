// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "jsx": "react",
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true,
    "baseUrl": "./src",     
    "paths": {
      "src/*": ["src/*"],
      "components/*": ["components/*"],
      "screens/*": ["screens/*"],
      "navigation/*": ["navigation/*"],
      "constants/*": ["constants/*"],
      "store/*": ["store/*"],
      "helpers/*": ["helpers/*"],
      "hooks/*": ["hooks/*"],
      "assets/*": ["assets/*"],
      "api/*": ["api/*"],
      "config/*": ["config/*"]
    },
    "types": [
      "node"
    ],                         
  }
}
